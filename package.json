{
  "name": "ccpm360-nextjs",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "pre-deploy": "node scripts/pre-deploy-check.js",
    "pre-deploy:quick": "npm run lint && npm run type-check && npm run build",
    "check": "npm run lint && npm run type-check",
    "ci-cd-check": "node scripts/ci-cd-precheck.js",
    "ci-cd-check:quick": "npm run lint && npm run type-check && npm run build",
    "pre-commit-enhanced": "npm run lint && npm run type-check && node scripts/check-dependencies.js",
    "pre-push-enhanced": "npm run ci-cd-check && npm audit --audit-level=high",
    "quality-check": "npm run pre-commit-enhanced && npm run build",
    "security-check": "npm audit --audit-level=moderate && node scripts/check-dependencies.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@mdx-js/react": "^3.1.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.1.7",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@sparticuz/chromium": "^138.0.2",
    "@supabase/supabase-js": "^2.54.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tiptap/extension-color": "^3.2.0",
    "@tiptap/extension-dropcursor": "^3.2.0",
    "@tiptap/extension-gapcursor": "^3.2.0",
    "@tiptap/extension-highlight": "^3.2.0",
    "@tiptap/extension-image": "^3.2.0",
    "@tiptap/extension-link": "^3.2.0",
    "@tiptap/extension-strike": "^3.2.0",
    "@tiptap/extension-text-style": "^3.2.0",
    "@tiptap/extension-underline": "^3.2.0",
    "@tiptap/pm": "^3.2.0",
    "@tiptap/react": "^3.2.0",
    "@tiptap/starter-kit": "^3.2.0",
    "@umami/node": "^0.4.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "handlebars": "^4.7.8",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "puppeteer": "^24.17.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/handlebars": "^4.0.40",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.19",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx}": [
      "prettier --write"
    ]
  }
}
